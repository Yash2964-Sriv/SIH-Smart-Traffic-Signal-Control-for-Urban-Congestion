<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">
    <input>
        <net-file value="intersection.net.xml"/>
        <route-files value="test_routes.rou.xml"/>
    </input>
    <time>
        <begin value="0"/>
        <end value="300"/>
        <step-length value="1"/>
    </time>
    <processing>
        <ignore-route-errors value="true"/>
        <ignore-junction-blocker value="true"/>
    </processing>
    <report>
        <verbose value="true"/>
        <no-step-log value="true"/>
        <statistics value="true"/>
        <statistics-output value="baseline_stats.xml"/>
    </report>
    <output>
        <fcd-output value="baseline_fcd.xml"/>
        <netstate-dump value="baseline_netstate.xml"/>
    </output>
</configuration>