<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
  <!-- vTypes for different vehicle types -->
  <vType id="car"   accel="1.8" decel="6.0" sigma="0.35" length="4" width="1.8" maxSpeed="16.67" guiShape="passenger" minGap="1.5" tau="1.2" lcStrategic="60" lcSpeedGain="40" lcKeepRight="0"/>
  <vType id="bus"   accel="1.0" decel="5.5" sigma="0.35" length="12" width="2.5" maxSpeed="13.89" guiShape="bus" minGap="2.5" tau="1.6" lcStrategic="30" lcSpeedGain="20"/>
  <vType id="truck" accel="0.7" decel="5.0" sigma="0.35" length="10" width="2.5" maxSpeed="12.0" guiShape="truck" minGap="2.5" tau="1.6" lcStrategic="30" lcSpeedGain="20"/>
  <vType id="auto"  accel="1.5" decel="5.5" sigma="0.45" length="3" width="1.3" maxSpeed="14.0" guiShape="motorcycle" minGap="1.0" tau="1.0" lcStrategic="60" lcSpeedGain="40"/>
  <vType id="bike"  accel="2.2" decel="5.0" sigma="0.45" length="2" width="0.8" maxSpeed="20.0" guiShape="motorcycle" minGap="0.9" tau="1.0" lcStrategic="70" lcSpeedGain="50"/>

  <!-- Corrected route definitions matching the actual network edge IDs -->
  <route id="S_to_N" edges="S_in N_out"/>
  <route id="S_to_E" edges="S_in E_out"/>
  <route id="S_to_W" edges="S_in W_out"/>

  <route id="N_to_S" edges="N_in S_out"/>
  <route id="N_to_E" edges="N_in E_out"/>
  <route id="N_to_W" edges="N_in W_out"/>

  <route id="E_to_W" edges="E_in W_out"/>
  <route id="E_to_N" edges="E_in N_out"/>
  <route id="E_to_S" edges="E_in S_out"/>

  <route id="W_to_E" edges="W_in E_out"/>
  <route id="W_to_N" edges="W_in N_out"/>
  <route id="W_to_S" edges="W_in S_out"/>

  <!-- Southbound: assign flows per lane to spread types -->
  <flow id="S_left_truck"  route="S_to_W" type="truck" begin="0" end="3600" period="18.0" departLane="0" departSpeed="4.0"/>
  <flow id="S_left_bus"    route="S_to_W" type="bus"   begin="0" end="3600" period="30.0" departLane="0" departSpeed="4.0"/>
  <flow id="S_left_car"    route="S_to_W" type="car"   begin="0" end="3600" period="8.0" departLane="0" departSpeed="6.0"/>

  <!-- Northbound: assign flows per lane to spread types -->
  <flow id="N_left_truck"  route="N_to_E" type="truck" begin="0" end="3600" period="20.0" departLane="0" departSpeed="4.0"/>
  <flow id="N_left_bus"    route="N_to_E" type="bus"   begin="0" end="3600" period="35.0" departLane="0" departSpeed="4.0"/>
  <flow id="N_left_car"    route="N_to_E" type="car"   begin="0" end="3600" period="9.0" departLane="0" departSpeed="6.0"/>

  <!-- Eastbound: assign flows per lane to spread types -->
  <flow id="E_left_truck"  route="E_to_S" type="truck" begin="0" end="3600" period="22.0" departLane="0" departSpeed="4.0"/>
  <flow id="E_left_bus"    route="E_to_S" type="bus"   begin="0" end="3600" period="40.0" departLane="0" departSpeed="4.0"/>
  <flow id="E_left_car"    route="E_to_S" type="car"   begin="0" end="3600" period="10.0" departLane="0" departSpeed="6.0"/>

  <!-- Westbound: assign flows per lane to spread types -->
  <flow id="W_left_truck"  route="W_to_N" type="truck" begin="0" end="3600" period="25.0" departLane="0" departSpeed="4.0"/>
  <flow id="W_left_bus"    route="W_to_N" type="bus"   begin="0" end="3600" period="45.0" departLane="0" departSpeed="4.0"/>
  <flow id="W_left_car"    route="W_to_N" type="car"   begin="0" end="3600" period="12.0" departLane="0" departSpeed="6.0"/>

  <!-- Straight through traffic -->
  <flow id="S_straight" route="S_to_N" type="car" begin="0" end="3600" period="6.0" departLane="0" departSpeed="8.0"/>
  <flow id="N_straight" route="N_to_S" type="car" begin="0" end="3600" period="7.0" departLane="0" departSpeed="8.0"/>
  <flow id="E_straight" route="E_to_W" type="car" begin="0" end="3600" period="8.0" departLane="0" departSpeed="8.0"/>
  <flow id="W_straight" route="W_to_E" type="car" begin="0" end="3600" period="9.0" departLane="0" departSpeed="8.0"/>

