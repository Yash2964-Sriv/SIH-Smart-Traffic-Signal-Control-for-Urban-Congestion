<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
  <!-- vTypes for different vehicle types -->
  <vType id="car"   accel="1.8" decel="6.0" sigma="0.35" length="4" width="1.8" maxSpeed="16.67" guiShape="passenger" minGap="1.5" tau="1.2" lcStrategic="60" lcSpeedGain="40" lcKeepRight="0"/>
  <vType id="bus"   accel="1.0" decel="5.5" sigma="0.35" length="12" width="2.5" maxSpeed="13.89" guiShape="bus" minGap="2.5" tau="1.6" lcStrategic="30" lcSpeedGain="20"/>
  <vType id="truck" accel="0.7" decel="5.0" sigma="0.35" length="10" width="2.5" maxSpeed="12.0" guiShape="truck" minGap="2.5" tau="1.6" lcStrategic="30" lcSpeedGain="20"/>
  <vType id="auto"  accel="1.5" decel="5.5" sigma="0.45" length="3" width="1.3" maxSpeed="14.0" guiShape="motorcycle" minGap="1.0" tau="1.0" lcStrategic="60" lcSpeedGain="40"/>
  <vType id="bike"  accel="2.2" decel="5.0" sigma="0.45" length="2" width="0.8" maxSpeed="20.0" guiShape="motorcycle" minGap="0.9" tau="1.0" lcStrategic="70" lcSpeedGain="50"/>

  <!-- Corrected route definitions based on actual network connections -->
  <!-- South to North (straight) -->
  <route id="S_to_N" edges="S_in S_out"/>
  <!-- South to West (left turn) -->
  <route id="S_to_W" edges="S_in W_out"/>
  <!-- South to East (right turn) -->
  <route id="S_to_E" edges="S_in E_out"/>

  <!-- North to South (straight) -->
  <route id="N_to_S" edges="N_in N_out"/>
  <!-- North to East (left turn) -->
  <route id="N_to_E" edges="N_in E_out"/>
  <!-- North to West (right turn) -->
  <route id="N_to_W" edges="N_in W_out"/>

  <!-- East to West (straight) -->
  <route id="E_to_W" edges="E_in E_out"/>
  <!-- East to South (left turn) -->
  <route id="E_to_S" edges="E_in S_out"/>
  <!-- East to North (right turn) -->
  <route id="E_to_N" edges="E_in N_out"/>

  <!-- West to East (straight) -->
  <route id="W_to_E" edges="W_in W_out"/>
  <!-- West to North (left turn) -->
  <route id="W_to_N" edges="W_in N_out"/>
  <!-- West to South (right turn) -->
  <route id="W_to_S" edges="W_in S_out"/>

  <!-- Vehicle flows based on real video analysis (829 vehicles, 41.3 vehicles/min) -->
  <!-- Southbound traffic -->
  <flow id="S_straight" route="S_to_N" type="car" begin="0" end="3600" period="8.0" departLane="0" departSpeed="6.0"/>
  <flow id="S_left" route="S_to_W" type="car" begin="0" end="3600" period="12.0" departLane="0" departSpeed="5.0"/>
  <flow id="S_right" route="S_to_E" type="car" begin="0" end="3600" period="15.0" departLane="0" departSpeed="7.0"/>

  <!-- Northbound traffic -->
  <flow id="N_straight" route="N_to_S" type="car" begin="0" end="3600" period="9.0" departLane="0" departSpeed="6.0"/>
  <flow id="N_left" route="N_to_E" type="car" begin="0" end="3600" period="13.0" departLane="0" departSpeed="5.0"/>
  <flow id="N_right" route="N_to_W" type="car" begin="0" end="3600" period="16.0" departLane="0" departSpeed="7.0"/>

  <!-- Eastbound traffic -->
  <flow id="E_straight" route="E_to_W" type="car" begin="0" end="3600" period="10.0" departLane="0" departSpeed="6.0"/>
  <flow id="E_left" route="E_to_S" type="car" begin="0" end="3600" period="14.0" departLane="0" departSpeed="5.0"/>
  <flow id="E_right" route="E_to_N" type="car" begin="0" end="3600" period="17.0" departLane="0" departSpeed="7.0"/>

  <!-- Westbound traffic -->
  <flow id="W_straight" route="W_to_E" type="car" begin="0" end="3600" period="11.0" departLane="0" departSpeed="6.0"/>
  <flow id="W_left" route="W_to_N" type="car" begin="0" end="3600" period="15.0" departLane="0" departSpeed="5.0"/>
  <flow id="W_right" route="W_to_S" type="car" begin="0" end="3600" period="18.0" departLane="0" departSpeed="7.0"/>

  <!-- Additional vehicle types for realism -->
  <flow id="S_truck" route="S_to_N" type="truck" begin="0" end="3600" period="25.0" departLane="0" departSpeed="4.0"/>
  <flow id="N_bus" route="N_to_S" type="bus" begin="0" end="3600" period="30.0" departLane="0" departSpeed="4.0"/>
  <flow id="E_auto" route="E_to_W" type="auto" begin="0" end="3600" period="20.0" departLane="0" departSpeed="8.0"/>
  <flow id="W_bike" route="W_to_E" type="bike" begin="0" end="3600" period="35.0" departLane="0" departSpeed="10.0"/>

